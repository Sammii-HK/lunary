{
  "framework": "nextjs",
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "installCommand": "yarn install",
  "crons": [
    {
      "path": "/api/cron/daily-posts",
      "schedule": "0 13 * * *"
    },
    {
      "path": "/api/cron/moon-packs?type=monthly",
      "schedule": "0 2 15 * *"
    },
    {
      "path": "/api/cron/moon-packs?type=quarterly", 
      "schedule": "0 3 15 1,4,7,10 *"
    },
    {
      "path": "/api/cron/moon-packs?type=yearly",
      "schedule": "0 4 1 7 *"
    }
  ]
}
