    "jazz-react": "^0.14.28",
    "jazz-tools": "^0.18.20",
    "lucide-react": "^0.546.0",
    "next": "15.5.4",
    "openai": "^6.8.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "react": "^18",
    "react-dom": "^18",
    "remark-gfm": "^4.0.1",
    "resend": "^6.1.2",
    "seed-random": "^2.2.0",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
