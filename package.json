    "@types/better-sqlite3": "^7.6.13",
    "@types/pg": "^8.15.5",
    "@types/web-push": "^3.6.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/blob": "^2.0.0",
    "@vercel/postgres": "^0.10.0",
    "astronomy-engine": "^2.1.19",
